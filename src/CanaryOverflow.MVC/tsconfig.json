{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./frontend/js/",
    "moduleResolution": "node",
    "allowJs": false,
    "sourceMap": true,
    "outDir": "./wwwroot/js/",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./frontend/js/**/*"
  ]
}